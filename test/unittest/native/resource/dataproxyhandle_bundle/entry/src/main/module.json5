{
  "module": {
    "name": "entry",
    "type": "entry",
    "description": "$string:module_desc",
    "mainElement": "EntryAbility",
    "deviceTypes": [
      "default"
    ],
    "deliveryWithInstall": true,
    "installationFree": false,
    "pages": "$profile:main_pages",
    "abilities": [
      {
        "name": "EntryAbility",
        "srcEntry": "./ets/entryability/EntryAbility.ts",
        "description": "$string:EntryAbility_desc",
        "icon": "$media:layered_image",
        "label": "$string:EntryAbility_label",
        "startWindowIcon": "$media:startIcon",
        "startWindowBackground": "$color:start_window_background",
        "exported": true,
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "ohos.want.action.home"
            ]
          }
        ]
      }
    ],
    "extensionAbilities": [
      {
        "srcEntrance": "./ets/entrybackupability/EntryBackupAbility.ts",
        "name": "DataShareExtAbility",
        "type": "dataShare",
        "uri": "datashare://com.acts.datasharetest",
        "readPermission": "ohos.permission.WIFI",
        "writePermission": "ohos.permission.WIFI",
        "exported": true
      },
      {
        "srcEntrance": "./ets/entrybackupability/EntryBackupAbility.ts",
        "name": "DataShareExtAbility2",
        "type": "dataShare",
        "uri": "datashare://com.acts.datasharetest2",
        "readPermission": "ohos.permission.WRITE_CONTACTS",
        "writePermission": "ohos.permission.GET_BUNDLE_INFO",
        "exported": true
      },
      {
        "srcEntrance": "./ets/entrybackupability/EntryBackupAbility.ts",
        "name": "DataShareExtAbility3",
        "type": "dataShare",
        "uri": "datashare://com.acts.testddd",
        "exported": true,
        "readPermission": "ohos.permission.WIFI",
        "writePermission": "ohos.permission.WIFI"
      }
    ],
    "proxyData":[
      {
        "uri": "datashareproxy://com.acts.datasharetest/test",
        "requiredReadPermission":"ohos.permission.GET_BUNDLE_INFO",
        "requiredWritePermission":"ohos.permission.WRITE_CONTACTS",
        "metadata": {
          "name": "dataProperties",
          "resource": "$profile:test"
        }
      },
      {
        "uri": "datashareproxy://com.acts.datasharetest/readtest",
        "requiredReadPermission":"ohos.permission.READ_CALL_LOG",
        "metadata": {
          "name": "dataProperties",
          "resource": "$profile:test"
        }
      },
      {
        "uri": "datashareproxy://com.acts.datasharetest/permissiontest/permission",
        "requiredWritePermission":"ohos.permission.WRITE_CALL_LOG",
        "metadata": {
          "name": "dataProperties",
          "resource": "$profile:test"
        }
      },
      {
        "uri": "datashareproxy://com.acts.datasharetest/entry/DB00/permission1",
        "requiredReadPermission":"ohos.permission.GET_BUNDLE_INFO",
        "requiredWritePermission":"ohos.permission.WRITE_CALL_LOG",
        "metadata": {
          "name": "dataProperties",
          "resource": "$profile:permission1"
        }
      },
      {
        "uri": "datashareproxy://com.acts.datasharetest/entry/DB00/permission2",
        "requiredReadPermission":"ohos.permission.GET_BUNDLE_INFO",
        "requiredWritePermission":"ohos.permission.WRITE_CALL_LOG",
        "metadata": {
          "name": "dataProperties",
          "resource": "$profile:permission2"
        }
      },
      {
        "uri": "datashareproxy://com.acts.datasharetest/entry/DB00/user",
        "requiredReadPermission":"ohos.permission.GET_BUNDLE_INFO",
        "requiredWritePermission":"ohos.permission.GET_BUNDLE_INFO",
        "metadata": {
          "name": "dataProperties",
          "resource": "$profile:user"
        }
      },
      {
        "uri": "datashareproxy://com.acts.datasharetest/entry/DB00/book",
        "requiredReadPermission":"ohos.permission.GET_BUNDLE_INFO",
        "requiredWritePermission":"ohos.permission.GET_BUNDLE_INFO",
        "metadata": {
          "name": "dataProperties",
          "resource": "$profile:book"
        }
      },
      {
        "uri": "datashareproxy://com.acts.datasharetest/entry/DB00/TBL00",
        "requiredReadPermission":"ohos.permission.GET_BUNDLE_INFO",
        "requiredWritePermission":"ohos.permission.GET_BUNDLE_INFO",
        "metadata": {
          "name": "dataProperties",
          "resource": "$profile:test"
        }
      }
    ],
    "crossAppSharedConfig": "$profile:cross_app_shared_config"
  }
}